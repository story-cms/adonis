{
  "name": "@story-cms/adonis",
  "version": "0.0.1",
  "description": "Story CMS package for the Adonisjs framework",
  "main": "./build/providers/StoryProvider.js",
  "typings": "./build/adonis-typings/index.d.ts",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/instructions.js",
    "build/templates"
  ],
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "types": "@story-cms/adonis",
    "providers": [
      "@story-cms/adonis"
    ],
    "templates": {
      "config": [
        "story.txt"
      ]
    }
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint . --ext=ts",
    "lint:fix": "eslint . --ext=ts --fix",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/story-cms/adonis.git"
  },
  "author": "OneSheep",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/story-cms/adonis/issues"
  },
  "homepage": "https://github.com/story-cms/adonis#readme",
  "peerDependencies": {
    "@adonisjs/auth": "^8.2.0",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/session": "^6.4.0"
  },
  "devDependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@adonisjs/session": "^6.4.0",
    "@onesheep/prettier-config": "^0.0.12",
    "@types/node": "^20.3.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.43.0",
    "mrm": "^4.1.14",
    "typescript": "^5.1.3"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": false
  },
  "prettier": "@onesheep/prettier-config"
}