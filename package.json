{
  "name": "@story-cms/adonis",
  "version": "0.0.1",
  "description": "Story CMS package for the Adonisjs framework",
  "main": "build/providers/StoryProvider.js",
  "scripts": {
    "lint": "tsc --noEmit && eslint . --ext=ts",
    "lint:fix": "eslint . --ext=ts --fix",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/story-cms/adonis.git"
  },
  "author": "OneSheep",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/story-cms/adonis/issues"
  },
  "homepage": "https://github.com/story-cms/adonis#readme",
  "peerDependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/auth": "^8.2.0"
  },
  "devDependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@adonisjs/session": "^6.4.0",
    "@types/node": "^20.3.1",
    "del-cli": "^5.0.0",
    "mrm": "^4.1.14",
    "typescript": "^5.1.3"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": false
  },
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ]
}